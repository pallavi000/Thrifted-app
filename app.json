{
  "expo": {
    "name": "Hamro Closet",
    "slug": "HamroCloset",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "enableDangerousExperimentalLeanBuilds": true,
      "package": "com.hamrocloset.HamroCloset"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "build": {
        "babel": {
          "include": [
            "rn-all-nepal-payment"
          ]
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "4d8cd9da-afaa-4a7d-a4a6-bb4f375c84f9"
      }
    }
  }
}
